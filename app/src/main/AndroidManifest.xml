<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/newlogo"
        android:label="@string/app_name"

        android:roundIcon="@drawable/newlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCal"
        tools:targetApi="31">
        <activity
            android:name="com.math.mycal.PerActivity"
            android:exported="false"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.WeightActivity"
            android:exported="false"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.LengthActivity"
            android:exported="false"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.BmiActivity"
            android:exported="false"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.GSTActivity"
            android:exported="true"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.MainActivity"
            android:exported="true"
            android:parentActivityName="com.math.mycal.HomeActivity"/>
        <activity
            android:name="com.math.mycal.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>